class Solution {
public:
    vector<int> nextGreaterElements(vector<int>& nums) {
        int n = nums.size();
        vector<int> v;

        for (int i = 0; i < n; i++) {
            bool found = false;
            int j = (i + 1) % n;

            while (j != i) {
                if (nums[j] > nums[i]) {
                    v.push_back(nums[j]);
                    found = true;
                    break;
                }
                j = (j + 1) % n;
            }

            if (!found) v.push_back(-1);
        }

        return v;
    }
};
